<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Project_1</display-name>

  <servlet>
    <display-name>login</display-name>
    <servlet-name>login</servlet-name>
    <servlet-class>com.revature.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>employeeProfile</display-name>
    <servlet-name>employeeProfile</servlet-name>
    <servlet-class>com.revature.servlets.EmployeeProfileServlet</servlet-class>
  </servlet>

  <servlet>
    <display-name>reimbursementidservlet</display-name>
    <servlet-name>ReimbursementIdServlet</servlet-name>
    <servlet-class>com.revature.servlets.ReimbursementIdServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>reimbursement</display-name>
    <servlet-name>reimbursement</servlet-name>
    <servlet-class>com.revature.servlets.ReimbursementServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>register</display-name>
    <servlet-name>register</servlet-name>
    <servlet-class>com.revature.servlets.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>homeManager</display-name>
    <servlet-name>ManagerHomeServlet</servlet-name>
    <servlet-class>com.revature.servlets.ManagerHomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManagerHomeServlet</servlet-name>
    <url-pattern>/manager</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>reimbursement</servlet-name>
    <url-pattern>/reimbursement</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReimbursementIdServlet</servlet-name>
    <url-pattern>/reimbursement/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>employeeProfile</servlet-name>
    <url-pattern>/employeeUpdate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>employeeProfile</servlet-name>
    <url-pattern>/employeeProfile/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>employeeProfile</servlet-name>
    <url-pattern>/managerProfile/*</url-pattern>
  </servlet-mapping>
</web-app>
